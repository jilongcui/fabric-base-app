{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/fabric-app/src",
  "compilerOptions": {
    "assets": [
      "**/*.proto"
    ],
    "watchAssets": true,
    "plugins": [
      {
        "name": "@nestjs/swagger",
        "options": {
          "classValidatorShim": true,
          "introspectComments": true
        }
      }
    ],
    "webpack": true,
    "tsConfigPath": "apps/fabric-app/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/fabric-app",
  "projects1": {
    "fabric-app": {
      "type": "application",
      "root": "apps/fabric-app",
      "entryFile": "main",
      "sourceRoot": "apps/fabric-app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/fabric-app/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "modules": {
      "type": "library",
      "root": "libs/modules",
      "entryFile": "index",
      "sourceRoot": "libs/modules/src",
      "compilerOptions": {
        "tsConfigPath": "libs/modules/tsconfig.lib.json"
      }
    },
    "shared": {
      "type": "library",
      "root": "libs/shared",
      "entryFile": "index",
      "sourceRoot": "libs/shared/src",
      "compilerOptions": {
        "tsConfigPath": "libs/shared/tsconfig.lib.json"
      }
    }
  },
  "projects": {
    "fabric-app": {
      "type": "application",
      "root": "apps/fabric-app",
      "entryFile": "main",
      "sourceRoot": "apps/fabric-app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/fabric-app/tsconfig.app.json"
      }
    }
  }
}